/*模糊查找弹出框*/
div#toastWindow {
	width: auto;
	height: auto;
	background: white;
	border: 1px solid #C5C5C5;
	border-radius: 5px;
	position: fixed;
	top: 27%;
	left: 60%;
	z-index: 99;
	display: none;
}

/*弹出框中的文本样式*/
#toastWindow div {
	min-width: 100px;
	font-size: 16px;
	height: 30px;
	line-height: 30px;
	color: #111111;
	text-align: left;
	/*margin: 5px 5px 0px;*/
	padding-left: 5px;
	padding-right: 5px;
	border-bottom: 1px dashed;
	cursor: pointer;
}

/*弹出选择框鼠标移动时的背景*/
#toastWindow div:hover {
	background: #F5F5F5;
}

