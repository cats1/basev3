.branding {
	width:100%;
    background: #2a2c2f;
    height: 40px;
    overflow: hidden;
}
.container{
	width: 1060px;
	margin:0 auto;
	height:auto;
	overflow: hidden;
}
.branding .navbar {
    margin-bottom: 0;
    border: none;
    background: none;
    width:100%;
    height:auto;
	overflow: hidden;
}
.branding .navbar a.navbar-brand {
    padding: 0;
    float: left;
    width: 3em;
    height:1.2em;
    margin:0.7em 0;
	overflow: hidden;
  cursor: default;
  margin-right: 0.6em
}
.branding .navbar a.navbar-brand img{
  width: 100%;
  height: auto
}
.branding .navbar ul.ex_list{
  list-style: none;
  width: auto;
  float: left;
  padding:0;
  margin-bottom: 0;
  height: 26px;
  line-height: 26px;
  margin-top: 7px
}
.branding .navbar ul.ex_list li{
  padding:0 30px;
  height:2em;
  float: left;
  color: #fff;
  font-size: 14px;
  pointer-events: auto;
  cursor: pointer;
  background: #272525
}
.branding .navbar ul.ex_list li:first-child{
  border-top-left-radius:1em;
  border-bottom-left-radius:1em;
}
.branding .navbar ul.ex_list li:last-child{
  border-top-right-radius:1em;
  border-bottom-right-radius:1em;
}
.branding .navbar ul.ex_list li.on{
  background: #666464
}
.branding .navbar .navmess{
	float: right;
	display: inline-block;
	width:auto;
	height:auto;
}
.branding .navbar .navmess .dropdown-toggle{
	display: block;
	width:auto;
	height:auto;
	text-decoration: none;
	color: #fff;
  cursor: default;
}
.branding .navbar .navmess .dropdown-toggle .com-logo {
    display: inline-block;
    width: 2.4em;
    float: left;
    overflow: hidden;
    margin-top: 19px;
}
.branding .navbar .navmess .dropdown-toggle .com-logo img {
    width: 100%;
}
.branding .navbar .navmess .dropdown-toggle .company {
	display: inline-block;
	float: left;
	font-size:1em;
	line-height: 64px;
	margin-left:0.5em;
}
