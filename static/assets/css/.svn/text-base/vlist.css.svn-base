.page-content1 .visitor-list li .people_detail{
  clear: both;
  height: auto;
  overflow: hidden;
  margin-left: 6em;
  padding-left: 15px;
  display: none;
  padding-top: 0.6em
}
.page-content1 .visitor-list li .people_detail .remark_text{
  border:1px solid #ccc;
  font-size: 0.8em;
  padding: 0.6em
}
.page-content1 .visitor-list li .people_detail p{
  font-size: 1em;
  line-height: 1.6em;
}
.page-content1 .visitor-list li .people_detail .people_list_p{
  border:1px solid #ccc;
  height: auto;
  overflow: hidden;
  margin-top: 0.8em;
  padding: 0.6em
}
.page-content1 .visitor-list li .people_detail ul.detail_list{
  list-style: none
}
.page-content1 .visitor-list li .people_detail ul.detail_list li{
    width: auto;
    height: auto;
    min-height: 0;
    padding: 0;
    background: #fff;
    float: left;
    margin: 0em;
    font-size: 0.8em;
    margin-right: 0.5em;
    border-radius:0;
    box-shadow: 0.5px 0.866px 4px 0px rgba( 25, 15, 12,0);
}
.confirm_win{
  position: fixed;
  width: 100%;
  height: 100%;
  background: rgba( 0, 0, 0,0.3);
  top: 0;
  left: 0;
  display: none
}

.confirm_win_center{
  position: absolute;
  width: 14em;
  height: 11em;
  background: #fff;
  top: 50%;
  left: 50%;
  margin-top: -5.5em;
  margin-left: -7em;
  z-index: 99999999;
  border-radius: 0.3em;
  box-shadow: 0.5px 0.866px 4px 0px rgba( 25, 15, 12,0.3);
}
.confirm_win_center .win_top{
  height:30px;
  background: #3986e2;
border-top-left-radius: 0.3em;
border-top-right-radius: 0.3em;
position: relative;
line-height: 30px
}
.confirm_win_center .win_top .closebtn{
  position: absolute;
  top: -2px;
  right: 16px;
  width: 16px;
  height: 16px;
  display: block;
  color: #000
}
.confirm_win_center .win_top .closebtn img{
  width: 100%;
}
.confirm_win_center .win_top p{
  color: #fff;
  text-align: center;
}
.confirm_win_center .win_remark{
  width: 90%;
  height: auto;
  margin:0 auto;
  overflow: hidden;

}
.confirm_win_center .win_remark label{
  width: 100%;
  height: auto;
  overflow: hidden;
  display: block;
  font-size: 0.8em
}
.confirm_win_center .win_remark textarea{
  width: 99%;
  height: auto;
  overflow: hidden;
  font-size: 0.8em;
  padding:6px 0;
  border:1px solid #ccc;
  margin:22px 0px 8px 0px;
}
.confirm_win_center .win_btns{
  width: 90%;
  height: auto;
  margin:0 auto;
  overflow: hidden;
}
.confirm_win_center .win_btns button{
  width: auto;
  padding:0.5em 2em;
  outline: none;
  border:0;
  border-radius: 4px;
  display: block;
  margin: 0 auto;
  font-size: 14px
}
.confirm_win_center .win_btns button.win_confirm{
  background: #2f8ef2;
  color: #fff
}
@media (max-width: 980px) {
  .container {
    width: 100%;
    margin:0;
    padding:0;
  }
  .branding .navbar a.navbar-brand {
    width:3em;
    height:1.2em;
    margin:0.7em 0.6em
  }
  .branding .navbar a.navbar-brand img{
  width:100%;
  }
  .branding .navbar .navmess {
    margin-top: 0em;
  }
  .page-content .row p.title {
    margin-top: 0.1em
  }
  .page-content .row p label{
    font-size: 0.8em
  }
  .page-content .row p label.details{
    margin-left: 0;
    font-size: 0.4em;
    line-height:1em
  }
  .page-content .row .order-wrapper {
  width:98%;
    margin:0 auto;
    padding:1em;
  }
  .page-content .order-wrapper ul.add-reservation-list li .form-group input {
    width: 80%;
  }
  .page-content .order-wrapper ul.add-reservation-list li .form-group .select-wrap .btn-group {
    width: 80%;
  }
  .page-content1 .visitor-list li .avatar{
    width: 6em;
    height: 6em;
  }
  .page-content1 .visitor-list li .wrap{
    width: auto;
  }
  .page-content1 .visitor-list li .wrap .visitor-header{
    padding-bottom:0;
    margin-bottom: 0
  }
  .page-content1 .visitor-list li .wrap .visitor-header .visitor{
    font-size: 1em;
    line-height: 1.6em
  }
  .page-content1 .visitor-list li .wrap .visitor-header .signinType{
    padding:4px 8px;
    margin-left: 5px;
    font-size: 0.8em;
    line-height: 1.2em
  }
  .page-content1 .visitor-list li .wrap .visitor-header .time{
    line-height: 2em;
    font-size: 0.6em;
    display: block;
    clear: both;
    float: none
  }
  .page-content1 .visitor-list li .wrap span{
    font-size: 0.8em;
    line-height: 1.8em
  }
}
.page-content1{
	display: block;
}
.page-content1 .visitype_con{
	width: 100%;
	height: auto;
	overflow: hidden

}
.page-content1 .visitype_con.on{
	display: block
}
.page-content1 .visitype_con .load_more{
  width: 90%;
  height: 2em;
  border:1px solid #6496ef;
  color: #6496ef;
  font-size: 0.8em;
  line-height: 2em;
  text-align: center;
  margin:0 auto;
  margin-bottom:20px;
}