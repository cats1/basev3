/* alertWindow */
.alert_hint{
  position: absolute;
  top: 0px;
  left: 0;
  width: 100%;
  z-index: 999999
}
.alert {
  margin-top: 0;
  font-size: 16px;
  line-height: 20px;
  border-radius: 0;
  color: #fff;
  border: none;
}
.alert .close {
  font-size: 16px;
  font-weight: normal;
  line-height: 20px;
  text-shadow: none;
  background: url(../img/close_small_white.png) no-repeat center;
  width: 20px;
  height: 20px;
  opacity: .8;
}
.alert .close:hover {
  opacity: .6;
}
.alert .close .glyphicon {
  display: none;
}
.alert .alert-link,
.alert a {
  color: #fff;
  border-bottom: 1px solid #fff;
  font-weight: normal;
}
.alert-danger {
  background: #f55567;
}
.alert-success {
  background: #67bf6b;
}
.alert-fixed {
  position: fixed;
  top: 0px;
  width: 100%;
  z-index: 9999;
  margin: 0;
}
.alertgo{
  color: #fff;
  padding:4px 10px;
  margin:0 20px;
  display: inline-block;
  border:1px solid #fff;  
}
