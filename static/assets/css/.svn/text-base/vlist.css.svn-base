/* button */
.green-button1{
  border-radius: 5px;
  border:1px solid #3fb26f;
  background-image: -moz-linear-gradient( 90deg, rgb(43,194,118) 0%, rgb(47,214,131) 100%);
  background-image: -webkit-linear-gradient( 90deg, rgb(43,194,118) 0%, rgb(47,214,131) 100%);
  background-image: -ms-linear-gradient( 90deg, rgb(43,194,118) 0%, rgb(47,214,131) 100%);
  padding:0 20px;
  font-size: 18px;
  font-family: "Microsoft YaHei";
  color: rgb( 255, 255, 255 );
  text-align: center;
  text-shadow: 0.5px 0.866px 0px rgba( 8, 1, 3 ,0.6);
}
.green-button1:hover{
  border-radius: 5px;
  border:1px solid #36995f;
  background-image: -moz-linear-gradient( 90deg, rgb(37,168,103) 0%, rgb(42,189,115) 100%);
  background-image: -webkit-linear-gradient( 90deg, rgb(37,168,103) 0%, rgb(42,189,115) 100%);
  background-image: -ms-linear-gradient( 90deg, rgb(37,168,103) 0%, rgb(42,189,115) 100%);
}
.green-button1:active{
  border: 1px solid rgb( 212, 212, 212 );
  border-radius: 5px;
  background-image: -moz-linear-gradient( 90deg, rgb(217,217,217) 0%, rgb(224,224,224) 100%);
  background-image: -webkit-linear-gradient( 90deg, rgb(217,217,217) 0%, rgb(224,224,224) 100%);
  background-image: -ms-linear-gradient( 90deg, rgb(217,217,217) 0%, rgb(224,224,224) 100%);
  color: #999999;
  text-align: center;
  text-shadow: 0.5px 0.866px 0px rgb( 255, 255, 255 );
}
.green-button1:focus{
  border-radius: 5px;
  background-image: -moz-linear-gradient( 90deg, rgb(43,194,118) 0%, rgb(47,214,131) 100%);
  background-image: -webkit-linear-gradient( 90deg, rgb(43,194,118) 0%, rgb(47,214,131) 100%);
  background-image: -ms-linear-gradient( 90deg, rgb(43,194,118) 0%, rgb(47,214,131) 100%);
color: rgb( 255, 255, 255 );
text-shadow: 0.5px 0.866px 0px rgba( 8, 1, 3 ,0.6);
