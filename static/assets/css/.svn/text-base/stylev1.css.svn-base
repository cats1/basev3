body.meet_room .bar{
  display: block
}
body.meet_room .roomwrap{
  width: 100%;
  background: #fff;
  padding:20px;
}
body.meet_room .meet_body{
  width: 100%;

}
body.meet_room .add_button_wrap{
  margin:10px 0;
  position: relative;
}
body.meet_room .searchsection .form-group{
  display: inline-block;
  float: left;
  width: auto;
  clear: none
}
body.meet_room .searchsection .form-group label{
  width: 80px;
  text-align: center;
  line-height: 37px
}
body.meet_room .searchsection .form-group input.form-control{
  width: 200px;
  line-height: 37px
}
body.meet_room .searchsection .form-group input.startdate,
body.meet_room .searchsection .form-group input.enddate{
  width: 150px;
  background: url(../img/v1/icon.png) 5px center no-repeat;
  text-align: center;
}

.roomtablewrap{
  border-radius: 4px;
  overflow: hidden;
  display: block;
  clear: both
}
.roomtablewrap table{
  width: 99.9%
}
.roomtablewrap table th{
  background: #318ce9;
  color: #fff;
  text-align: center;
  font-size: 16px;
  line-height: 50px;
  border:1px solid #318ce9;
}
.roomtablewrap table tr{
 height: 37px;
 line-height: 37px;
 color: #666;
 text-align: center;
}
.roomtablewrap table tbody tr:nth-child(2n+1){
  background: #fff
}
.roomtablewrap table tbody tr:nth-child(2n){
  background: #c0ddfb
}
.roomtablewrap table td{
  border:1px solid #318ce9;
}
.bluelink{
  color: #318ce9;
  cursor:pointer;
}
.searchsection .scroll-mainBox{ 
  width:200px;
  height:76px; 
  border:1px #bbb solid;
  background: #fff;
  position:absolute; 
  top: 2px;
  left: 80px;
  z-index: 9999;
  overflow:hidden; 
  margin:50px auto;
}
.searchsection .scroll-content{  
  height:auto;
  width: 100%; 
  position:absolute; 
  left:0; 
  top:0; 
}
.searchsection .scrollDiv,#scrollDiv{ 
  width:8px;  
  position:absolute; 
  top:0; 
  background:#666; 
  border-radius:10px;
}
.searchsection ul.scroll_list{
  width: 100%
}
.searchsection ul.scroll_list li{
  width: 100%;
  cursor: pointer;
  height: 37px;
  line-height: 37px;
  font-size: 14px;
  color: #444;
  text-indent: 24px;
}
.searchsection ul.scroll_list li:hover{
  background: #666;
  color: #fff
}
@media (max-width: 1060px) {
    .container {
        width: 100%;
        min-width: 1060px;
    }
}
