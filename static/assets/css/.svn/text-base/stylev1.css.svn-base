body {
    font-family: "microsoft yahei","Arial";
    background: #e7e8e9;
    font-size:16px;
    color: #e7e8e9;
    margin:0;
    padding: 0;
    -webkit-font-smoothing: antialiased;
}
/* header */
.branding {
  background: #2a2c2f;
}
.branding .hint {
  font-size: 16px;
  float: right;
  margin-top: 39px;
  line-height: 20px;
  margin-right: 35px;
}
.branding .hint span {
  color: #fff;
  opacity: .7;
}
.branding .hint a {
  color: #fff;
  font-weight: bold;
  border-bottom: 1px solid #fff;
  opacity: .8;
}
.branding .hint a:hover {
  opacity: 1;
}
.branding .logo {
  margin: 0;
}
.branding .navbar-default .navbar-collapse,
.branding .navbar-default .navbar-form,
.branding .navbar-default {
  border: none;
  background: none;
}
.branding .navbar-nav > li {
  float: left;
  height: 66px;
  line-height: 66px;
  position: relative;
  text-align: center
}
.branding .navbar-nav li.customize img {
  position: absolute;
  bottom: 0;
  width: 10px;
  height: 4px;
  left: 50%;
  margin-left: -5px;
  display: none
}
.branding .navbar-nav li.active{
  background: #33373f;
  border-bottom: 6px solid #308ce8;
}
.branding .navbar-nav:not(.navbar-right) li:not(.active):hover{
  background: #33373f;
}
.branding .navbar-nav > li > a {
  padding: 20px 0;
  font-size: 18px;
  margin-right: 50px;
}
.branding .navbar-default .navbar-nav > li > a {
  opacity: 1;
  color: #fff;
  font-size: 18px;
  width: 100%;
  text-align: center
}
.branding .navbar-default .navbar-nav > li > a:hover {
  opacity: 1;
}
.branding .navbar-default .navbar-nav > .active > a,
.branding .navbar-default .navbar-nav > .active > a:hover,
.branding .navbar-default .navbar-nav > .active > a:focus {
  opacity: 1;
  color: #fff;
  background: none;
  font-weight: bold;
}
.branding .navbar {
  margin-bottom: 0;
}
.branding a.navbar-brand {
  color: #fff;
  height: 64px;
  width: 134px;
  background: url(../img/v1/v1logo.png) no-repeat left;
  text-indent: -999em;
  padding: 0;
}
.branding .versionrecord{
  width: auto;
  display: inline-block;
  float: left;
  margin-top: 14px;
  margin-right: 24px;
  position: relative;
  cursor: pointer;
}
.branding .versionrecord .versionleft{
	height: 22px;
	width: auto;
	background: url(../img/v1/versionl.png) left center no-repeat;
	padding-left: 10px;
}
.branding .versionrecord .versionleft .versionright{
	height: 22px;
	width: auto;
	background: url(../img/v1/versionr.png) right center no-repeat;
	padding-right: 10px;
}
.branding .versionrecord .versionleft .versionright .versionmiddle{
	height: 22px;
	line-height: 22px;
	min-width: 30px;
	width: auto;
	text-align: center;
	background: url(../img/v1/versionm.png) left center repeat-x;
	font-size: 14px;
	color: #fff
}
.branding .navbar-right > li {
  border-left: 1px solid #373a3f;
  padding:0 15px
}
.branding .navbar-right li.dropshow {
  background: #33373f
}
.branding .navbar-right li.dropshow .caret {
    display: inline-block;
    width: 0;
    height: 0;
    margin-left: 2px;
    vertical-align: middle;
    border-top: 0;
    border-left: 4px solid transparent;  
    border-right: 4px solid transparent;    
    border-bottom: 4px solid;
}
.branding .navbar-right li:last-child {
  border-right:1px solid #373a3f;
}
.branding .navbar-right #front_link a{
  font-size: 14px;
  height: 25px;
  line-height: 25px
}
.branding .navbar-right > li > a {
  margin-right: 0;
  padding: 0;
  height: 25px;
  line-height: 25px;
  margin:20px 0;
  opacity: 1 !important;
}
.branding .navbar-default .navbar-nav > .open > a,
.branding .navbar-default .navbar-nav > .open > a:hover,
.branding .navbar-default .navbar-nav > .open > a:focus {
  background: transparent;
  color: #fff;
  opacity: 1;
}
.branding .navbar-right .dropdown-menu {
  border-bottom-left-radius: 3px;
  min-width: 0;
  font-size: 16px;
  overflow: hidden;
  border:none;
  box-shadow: 0 0px 0px rgba(0,0,0,0);
}
.branding .navbar-right .dropdown-menu li{
  background: #33373f;
}
.branding .navbar-right .dropdown-menu li:hover{
  background: #308ce8; 
}
.branding .navbar-right .dropdown-menu a {
  color: #ffffff;
  padding: 10px 20px;
}
.branding .navbar-right .dropdown-menu a:hover {
  background: #308ce8; 
}
.branding .dropdown-menu {
  padding: 0;
}
.branding .dropdown .com-logo {
  display: inline-block;
  width: 25px;
  height: 25px;
  float: left;
  margin-right: 3px;
}
.branding .dropdown .com-logo img {
  max-width: 100%;
  height: auto;
}
.branding .dropdown .logocom{
  display: inline-block;
  float: left;
  width:auto;
  max-width: 150px;
  margin-bottom: 0;
  margin:0 4px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-size: 14px;
}
.page-content {
  min-height: 600px;
  padding-top: 20px;
  background: transparent;
  position: relative;
  overflow: hidden;
  padding-bottom: 20px
}
.colophon{
	width: 100%;
	height: auto;
	background: #2a2c2f;
	overflow: hidden;
	padding:30px 0
}
.colophon p{
	font-size: 14px;
	color: #fff;
	text-align: center;
	line-height: 26px;
}
.container {
  width: 1060px;
  height: auto;
  position: relative;
}
.bar {
    min-height: 54px;
    background: #fff;
    font-size: 16px;
    display: none;
    position:relative;
}
.bar .nav{
    position: relative;
}
.bar ul{
  width: auto;
  height: 54px;
  float: left;
  overflow: hidden;
  position: absolute;
  left: 50%;
  margin-left: -188px
}
.bar ul li{
  float: left;
  height: 54px;
  line-height: 54px
}
.bar ul li a{
  font-size: 14px;
  color: #444;
  width: 100%;
  height: 100%;
  padding:0 20px
}
.bar ul li:hover a{
  color: #2e87df
}
.bar ul li.active{
  border-bottom:4px solid #308ce8
}
.bar ul li.active a{
  color: #2e87df;
}
/* index */
.page-content .page-header{
  border:none;
  width: 100%;
  margin: 20px 0 20px;
  position: relative;
}
.page-content .page-header h1{
  margin: 5px 0;
  font-size: 30px;
  color: #666
}
.page-content .page-header .sub {
    font-size: 14px;
    color: #666;
}

/* 普通账号 */
