body {
  background: url(https://www.coolvisit.com/assets/img/invibg.jpg) left top repeat;
  color: #353d4c;
  font: normal 100% "microsoft yahei",Helvetica, Arial, sans-serif;
  -webkit-font-smoothing: antialiased;
}
.container{
	position: relative;
	width:90%;
	margin:0 auto;
  padding:0;
	border-radius: 5px;
  	background-color: rgb( 255, 255, 255 );
  	box-shadow: 2.5px 4.33px 15px 0px rgb( 36, 44, 53 );
  	height:auto;
  	overflow: hidden;
}
.container .header{
	width:100%;
	height:auto;
  	overflow: hidden;
}
.container .header img{
  width: 100%;
  height: auto
}
.container .footer{
	width:100%;
	height:auto;
  	overflow: hidden;
    position: absolute;
    bottom: -4px
}
.container .footer img{
  width: 100%;
  height: auto
}
.container section{
  width: 90%;
  height: auto;
  margin:10px auto;
}
.container section.s1 .timecons{
  width: 80%;
  text-indent: 28px;
  margin:0 auto;
  border-bottom: 1px dashed #acb3b9;
  height: 3em;
  position: relative;
  line-height: 2.5em
}
.container section.s1 .timecons span{
  padding-left: 15px
}
.container section.s2 h2{
height: auto;
    overflow: hidden;
    background: url(https://www.coolvisit.com/assets/img/v1/inline2.png) center center no-repeat;
    background-size: 100%;
    font-size: 1.2em;
    color: #40b4c4;
    text-align: center;
}
.container section.s2 .mapcon{
  width: 100%;
    height: 310px;
    position: relative;
    margin: 30px 0;
    clear: both;
    top: 20px;
}
.container section.s2 .mapcon .map{
overflow: hidden;
height: 100%;
    position: relative;
    z-index: 0;
    color: rgb(0, 0, 0);
    text-align: left;
    background-color: rgb(243, 241, 236);
}
