}
.form-group label {
  width: auto;
  display: inline-block;
  float: left;
  font-size: 14px;
  color: #444
}
.form-control {
  border-radius: 0;
  box-shadow: none;
  width: 270px;
  display: inline-block;
  float: left;
  font-size: 14px;
  height: 37px;
  line-height: 37px;
  color: #999
}
.form-control:focus {
  box-shadow: none;
}
.form-group .form-control.bgnone{
  width: auto;
  min-width: 40px;
  padding: 0 10px;
  background: none;
  border:none;
  cursor: default;
