}
/* 标签定义 */
div,a,p,section,nav,h1,h2,h3,h4,ul,ul li,dl,dl dt,dl dd,label,strong{
  margin: 0;
  padding: 0;
  font-family: 'microsoft yahei';
}

a {
  text-decoration: none;
  color: #333
}
a:hover{
  color: #333
}
section {
  height: auto;
  overflow: hidden;
}

ul,ul li {
  list-style: none;
}
button{
  outline: none;
  border:none;
}
/* common */
h2.title{
  font-size: 18px;
  color: #444
}
a.help{
  color: #308ce8;
  font-size: 14px
}
label.gray1{
  font-size: 18px;
  color: #333
}
.anchorBL {
    display: none;
}
.pointer-none{
  pointer-events: none;
  opacity: 0.6
}
.pointer-none1{
  pointer-events: none
}
.left {
  float: left;
}
.right {
  float: right;
}
.check {
  border-radius: 2px;
  width: 83px;
  height: 36px;
  overflow: hidden;
  position: relative;
  cursor: pointer;
}
.check .bg {
  color: #fff;
  text-shadow: 0.5px 0.866px 0px rgba( 8, 1, 3,.3);
}
.check .bg .left{
  width: 50%;
  height: 36px;
  text-align: center;
  line-height: 36px;
}
.check .bg .right{
  width: 50%;
  height: 36px;
  text-align: center;
  line-height: 36px;
}
.check .switch {
  height: 30px;
  width: 41px;
  border-radius: 1px;
  background-color: rgb( 255, 255, 255 );
  box-shadow: 0.5px 0.866px 4px 0px rgba( 8, 1, 3,0.3);
  position: absolute;
  margin:3px;
  left: 0;
}
.check.checked {
  background: #308ce8;
}
.check.checked .switch {
  left: 35px;
}
.check.unchecked {
  background: #b5b7ba;
}
.check.unchecked .switch {
  left: 0px;
}
.hint{
  color: #666
}
.tipshint{
  color: #666;
  font-size: 14px;
  line-height: 36px
}
.example{
  padding: 10px;
  color: #ca9820;
  background: #fff4bf;
  font-size: 12px;
  margin:10px 0;
  border: 1px solid #f3ca4f;
}
.row{
  margin:0
}
.form-group {
  position: relative;
  clear: both;
  height: auto;
  overflow: hidden;
  margin:16px 0
}
.form-group .error-msg {
  position: absolute;
  right: 15px;
  bottom: 14px;
  color: #d97f7f;
}
.form-group .row .error-msg {
  right: 30px;
}
.form-group .row .warning-msg {
  right: 30px;
}
.form-group .warning-msg {
  position: absolute;
  right: 15px;
  bottom: 14px;
  color: #c19a23;
}
.form-group .form-control.error {
  border-color: #d97f7f;
}
.form-group .form-control.warning {
  border-color: #c19a23;
}
.form-control[disabled],
.form-control[readonly],
fieldset[disabled] .form-control {
  cursor: not-allowed;
  background-color: #f5f5f5;
  opacity: 1;
}
.btn{
  background: #308ce8;
  outline: none;
  border:none;
  height: 37px;
  line-height: 37px;
  border-radius: 0px
}
.btn-default {
  color: #444;
  background: #fff;
  border: none;
  outline: none !important;
  height: 37px;
  line-height: 37px;
  padding: 0 10px;
  font-size: 14px;
}
.btn-default:hover,
.btn-default:focus {
  color: #444;
  background: #fff;
}
.btn-blue {
  color: #fff;
  background: #308ce8;
  border: none;
  outline: none !important;
  height: 37px;
  line-height: 37px;
  padding: 0 10px;
  font-size: 14px;
}
.btn-group.open .dropdown-toggle{
  -webkit-box-shadow: inset 0 3px 5px rgba(0,0,0,0);
  box-shadow: inset 0 3px 5px rgba(0,0,0,0);
}
.btn-default:hover, .btn-default:focus, .btn-default.focus, .btn-default:active, .btn-default.active, .open>.dropdown-toggle.btn-default{
  background: transparent;
}
.btn-bordered {
  border: 1px solid #67bf6b;
  color: #67bf6b;
  background: none;
}
.btn-bordered:hover,
.btn-bordered:focus {
  background: none;
