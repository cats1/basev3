.tooltip.bottom {
  margin-top: 9px;
}
.tooltip.bottom .tooltip-arrow {
  width: 18px;
  height: 9px;
  background: url(../img/arrow.png) no-repeat center;
  border: none;
  margin-left: -9px;
}
.tooltip.bottom .tooltip-inner {
  background: rgba(0, 0, 0, 0.9);
  border-radius: 0;
  font-size: 14px;
  margin-top: 4px;
  padding: 10px 15px;
}
.modal {
    position: fixed;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    z-index: 1040;
    display: none;
    outline: 0;
}
.modal .modal-backdrop.in {
  opacity: .35;
}
.modal .modal-dialog {
  width: 700px;
  margin: 130px auto;
  background: rgba(0,0,0,0.4);
  padding:16px;
}
.modal .modal-dialog .modal-content {
  padding: 0;
  border-radius: 0;
}
.modal .modal-dialog .modal-content p {
  font-size: 16px;
}
.modal .modal-dialog .modal-header {
  padding: 0;
  border-bottom: none;
  width:100%;
  height:49px;
  background-color: rgb( 224, 224, 224 );
}
.modal .modal-dialog .modal-header .close {
  position: absolute;
  background: url(../img/close_small.png) no-repeat center;
  background-size:100% 100%; 
  width: 14px;
  height: 14px;
  opacity: 1;
  outline: 0;
  right: 30px;
  top: 18px;
}
.modal .modal-dialog .modal-header .close span {
  display: none;
}
.modal .modal-dialog .modal-header .close:hover {
  opacity: .7;
}
.modal .modal-dialog .modal-content .modal-header h4{
  line-height: 49px;
  text-align: center;
  font-size:18px;
  color: #444
}
.modal .modal-dialog .modal-body {
  padding: 0;
  background: #fff;
  min-height: 50px;
  padding:50px 0;
}
.modal .modal-dialog .modal-body img {
  max-width: 100%;
}
.modal .modal-dialog .modal-content .modal-body h4{
  font-size:14px;
  color:#999;
}

a[data-toggle='modal'] {
  outline: 0;
}
