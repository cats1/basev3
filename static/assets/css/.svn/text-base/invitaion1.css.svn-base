body {
  background: url(https://www.coolvisit.com/assets/img/invibg.jpg) left top repeat;
  color: #353d4c;
  font: normal 100% "microsoft yahei",Helvetica, Arial, sans-serif;
  -webkit-font-smoothing: antialiased;
}
img{
	border:0;
}

div#codewrap img {
				border: 1px solid #EAEAEA;
				padding: 15px;
}
.container{
	position: relative;
	width:760px;
	margin:0 auto;
	border-radius: 5px;
  	background-color: rgb( 255, 255, 255 );
  	box-shadow: 2.5px 4.33px 15px 0px rgb( 36, 44, 53 );
  	height:auto;
  	overflow: hidden;
}
p.pdetail{
	width:760px;
	margin:0 auto;
	text-align: center;
	line-height: 2em;
	color:#cccccc;
}
.container .header{
	width:100%;
	height:auto;
  	overflow: hidden;
}
.container .header img{
	width:100%;
	height:auto;
}
.container .footer{
	width:100%;
	height:auto;
  	overflow: hidden;
}
.container .footer img{
	position: relative;
	bottom: -4px;
	width:100%;
	border:0;
}
.container section{
	height:auto;
	overflow: hidden;
}
.container section.s3{
	width: 80%;
	margin:0 auto;
}
.container section .con1{
	width: 80%;
	margin:0 auto;
}
.container section .con1 img{
	max-width: 100%
}
.container section.s1 p{
	font-size: 0.875em;
	color:#353d4c;
	line-height: 2em;
	text-align: justify;
}
.container section.s1 .timecons{
	width: 80%;
	text-indent: 28px;
	margin:0 auto;
	border-bottom: 1px dashed #acb3b9;
	height: 3em;
	position: relative;
}
.container section.s1 .timecons span.timecon{
	position: absolute;
	margin: 1em;
	margin-left: 0;
	font-size: 0.9em;
	color:#666
}
.container section.s1 p.steps{
	font-size: 0.875em;
	color:#353d4c;
	line-height: 2.5em;
	text-indent: 24px;
}
.container section.s1 p.steps i{
	margin-right: 20px;
}
.container section.s1 .con1 p{

}
.container section h2{
	height:auto;
	overflow: hidden;
	background: url(https://www.coolvisit.com/assets/img/v1/inline2.png) center center no-repeat;
	background-size:80% ;
	font-size: 1.2em;
	color:#40b4c4;
	text-align: center
}
.container section .descsection h2 {
	background: none;
}
.container section .descsection.codescanner{
	margin-top: 10px
}
.container section .descsection .codewrap{
	width: 14em;
	height: 14em;
	padding:3px;
	margin:26px auto 0 auto;
	border:1px solid #ccc
}
.container section p{
	width:100%;
	margin:0 auto;
	text-align: justify;
	font-size:0.875em;
	line-height: 1.5em;
	color:#353d4c;

}
.container section.s2 p{
	width: 80%;
	margin:0 auto;
}
.container section.s2 .mapwrap{
	width:80%;
	height:26em;
	background: #fff;
	margin:0em auto;
	position: relative;
}
.container section.s2 .mapwrap .mapcon{
	position: relative;
	top: 0;
	left: 0;
	width:80%;
	height:20em;
	margin:1em auto;
	background: transparent;
	border-radius: 2px;
	z-index: 10;
}
.container section.s2 .mapwrap .mapmask{
	position: absolute;
	left: 0;
	top: 0;
	z-index: 11;
	width: 100%;
	height:24em;
	margin:0em auto;
}
.container section.s2 .mapwrap .mapmask img{
	display: block;
	margin: 0 auto;
	width:80%;
}
.container section.s2 .mapwrap .mapcon .map{
	position: absolute; 
    width: 98%; 
    height: 98%;
    top: 1%;
    left:1%;
    overflow:hidden;   
}

.container section.s2 .mapwrap .mapcon .map .anchorBL{
	display: none
}
.container section.s2 .gohere{
	display: block;
	width:7em;
	height:3em;
	margin:0 auto;
	color:#fff;
	text-decoration: none;
	line-height: 3em;
	font-size: 16px;
    font-family: "Microsoft YaHei";
    text-align: center;
    text-shadow: 0.5px 0.866px 0px rgba( 8, 1, 3,0.2);
	background: #40b4c4;
}

.appointment {
	width:7em;
	height:7em;
	margin:0 auto;
	color:#fff;
	background: url(https://www.coolvisit.com/assets/img/appointment.png) no-repeat;
}

.container section p.compro{
	text-align: justify;
}
#upload_image{
	padding-bottom: 0.8em!important
}
@media (max-width: 980px) {
  .container {
    width: 100%;
  }
  p.pdetail{
  	width: 100%;
  }
  .container section h2{
	height:auto;
	overflow: hidden;
	background: url(https://www.coolvisit.com/assets/img/v1/inline2.png) center center no-repeat;
	background-size:100% ;
	font-size: 1.2em;
	color:#40b4c4;
	text-align: center
}
.container section .descsection h2 {
	height: auto;
    overflow: hidden;
    background: url(https://www.coolvisit.com/assets/img/v1/inline2.png) center center no-repeat;
    background-size: 100%;
    font-size: 1.2em;
    color: #40b4c4;
    text-align: center;
}
}

#fixed-buttonGroup button {
					width: 47%;
					height: 40px;
					margin:15px 5px;
					border-radius: 4px;
					border: transparent;
					color: white;
					font-size: 16px;
					letter-spacing: 3px;
}

#blueGradients {
				background-color: #1872CD;
				float: left;
}

#redGradients {
				background-color: #E65234;
				float: right;
}

.grayGradients {
				background-color: #CCCCCC;
}

