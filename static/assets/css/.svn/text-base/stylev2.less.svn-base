.content-left {
    width: 26%;
    display: inline-block;
    float: left;
    .search-wrap {
        line-height: 48px;
        padding: 10px 0;
        input {
            width: 90%;
            margin: 0 auto;
            display: block;
            line-height: 36px;
            color: #000;
        }
    }
    .pro-content-wrap {
        height: auto;
        background: #f2f3f4;
    }
}

.photo-wrap {
    background: #ccc;
    width: 86px;
    height: 120px;
    position: relative;
    border-radius: 4px;
    text-align: center;
    margin: 0 auto;
    overflow: hidden;
    img {
        width: 60%;
        height: auto;
        margin: 10px auto;
    }
    img.onloadphoto {
        width: 100%;
        height: 100%;
        margin: 0;
        position: absolute;
        left: 0;
        top: 0;
    }
    input {
        position: absolute;
        width: 100%;
        height: 100%;
        top: 0;
        left: 0;
        opacity: 0
    }
    p {
        color: #000;
        line-height: 32px;
    }
}

.c_input {
    background-color: #fff;
    background-image: none;
    border: 1px solid #ccc;
    border-radius: 4px;
    height: auto;
    min-height: 37px;
    line-height: 25px;
    color: #555;
    padding: 6px;
    span {
        position: relative;
        padding: 5px 5px;
        background: #eceff4;
        border-radius: 3px;
        color: #333333;
        text-align: center;
        font-size: 12px;
        margin-right: 3px;
        margin-top: 1px;
        margin-bottom: 1px;
    }
}

body.employees .bar {
