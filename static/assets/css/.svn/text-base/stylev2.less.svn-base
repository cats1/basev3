.search-table-wrap {
    width: 96%;
    margin: 10px auto;
    overflow: auto;
    border: 1px solid #c7c7c7;
    border-bottom: none;
    table {
        width: auto;
        th {
            background: #6897ec;
            color: #fff;
            font-size: 14px;
            line-height: 40px;
            border-right: 1px solid #c7c7c7;
            text-align: center;
            padding: 0 20px;
            white-space: nowrap;
            &:last-child {
                border-right: none;
            }
            &:first-child {
                width: 200px
            }
        }
        tr {
            border-bottom: 1px solid #c7c7c7;
        }
        tbody {
            tr:nth-child(2n+1) {
                background: #fff
            }
            tr:nth-child(2n) {
                background: #f7f7f7
            }
        }
        td {
            color: #666;
            font-size: 14px;
            line-height: 40px;
            border-right: 1px solid #c7c7c7;
            text-align: center;
            padding: 0 20px;
            white-space: nowrap;
            &:last-child {
                border-right: none;
            }
        }
    }
    .rvwrap {
        white-space: nowrap;
        clear: both;
        min-width: 80px;
    }
    .vlogowrap {
        width: 28px;
        height: 28px;
        display: inline-block;
        float: left;
        background: #6897ec;
        border-radius: 50%;
        margin: 6px 6px 0 0;
        overflow: hidden;
        img {
            width: 100%;
            height: 100%;
            vertical-align: top;
        }
    }
    .vcardwrap {
        width: 42px;
        height: 32px;
        display: inline-block;
        float: left;
        background: #6897ec;
        margin: 6px 6px 0 0;
        overflow: hidden;
        border-radius: 4px;
        img {
            width: 100%;
            height: 100%;
            vertical-align: top;
        }
    }
    .vlname {
        display: inline-block;
        float: left;
    }
}

