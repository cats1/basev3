.approvebody {
    .page-content {
        .container {
            background-color: rgb(255, 255, 255);
            box-shadow: 0px 1px 4px 0px rgba(183, 189, 193, 0.004);
            padding: 0;
            h2 {
                font-size: 18px;
                color: rgb(102, 102, 102);
                line-height: 48px;
                border-bottom: 1px solid #cecece;
                text-indent: 24px;
            }
            .section-wrap {
                padding: 20px 24px;
                .regions {
                    border-width: 1px;
                    border-color: rgb(181, 181, 181);
                    border-style: solid;
                    background-color: rgb(246, 246, 246);
                    margin: 10px 0;
                    .region-logo {
                        display: inline-block;
                        overflow: hidden;
                        img {
                            float: left;
                            margin: 10px 0;
                        }
                        ul {
                            float: left;
                            margin: 24px 0 0 20px;
                            li {
                                float: left;
                                margin: 0 10px 0 0;
                                button {
                                    background-color: rgb(58, 129, 199);
                                }
                            }
                        }
                        strong {
                            float: left;
                            color: #444444;
                            text-indent: 24px;
                            line-height: 80px;
                            font-weight: lighter;
                        }
                    }
                    .region-cons {
                        margin-left: 20px;
                        strong {
                            font-size: 24px;
                            color: #444444
                        }
                    }
                }
                .regions-top {
                    border-bottom: 1px rgb(181, 181, 181) solid;
                    background-color: rgb(246, 246, 246);
                    h3 {
                        line-height: 36px;
                        color: #444444;
                        padding: 0 24px;
                        position: relative;
                        strong {
                            font-size: 18px;
                        }
                        i {
                            font-size: 14px;
                            font-style: normal;
                            margin-left: 18px;
                        }
                        span {
                            display: none;
                            cursor: pointer;
                            &.closearea {
                                float: right;
                            }
                            &.editarea {
                                position: absolute;
                                right: 50px;
                                top: 0;
                            }
                        }
                    }
                }
                .regions-body {
                    background-color: rgb(255, 255, 255);
                    padding: 18px 18px 10px;
                }
                .add-regions {
                    cursor: pointer;
                }
            }
        }
    }
}

.rightmodal {
    width: auto;
    min-width: 400px;
    height: 100%;
    position: fixed;
    right: 0;
    top: 0;
    display: none;
    z-index: 9999;
    .rightmodal-dialog {
        width: auto;
        min-width: 400px;
        height: 100%;
        background: #fff;
        background-color: rgb(255, 255, 255);
        box-shadow: 0px 2px 10px 0px rgba(3, 0, 0, 0.6);
        position: absolute;
        right: 0;
        .rightmodal-content {
            width: auto;
            height: 100%;
            position: relative;
            .rightmodal-header {
                color: #000;
                background-color: rgb(223, 239, 255);
                height: 50px;
                font-size: 24px;
                color: rgb(0, 0, 0);
                line-height: 50px;
                text-align: center;
                span {
                    width: 49.9%;
                    text-align: center;
                    display: inline-block;
                    float: left;
                    &:first-child {
                        border-right: 1px solid #c9cdd0;
                    }
                }
            }
            .rightmodal-body {
                padding: 0;
                .areas {
                    width: 49.9%;
                    display: inline-block;
                    float: left;
                    height: 100%;
                    &:first-child {
                        border-right: 1px solid #c9cdd0;
                    }
                }
                .form-group {
                    height: auto;
                    overflow: inherit;
                }
                label {
                    width: 30%;
                    text-align: right;
                    line-height: 37px;
                    padding: 0
                }
                input {
                    width: 100%;
                    padding: 6px;
                    background-color: #fff;
                    background-image: none;
                    border: 1px solid #ccc;
                    border-radius: 4px;
                    min-height: 28px;
                    box-sizing: border-box;
                    -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075);
                    box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075);
                    -webkit-transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;
                    transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;
                    &::-webkit-input-placeholder {
                        /* placeholder颜色  */
                        color: #aab2bd;
                        /* placeholder字体大小  */
                        font-size: 12px;
                        /* placeholder位置  */
                        text-align: left;
                    }
                    &:-moz-placeholder{
                        /* placeholder颜色  */
                        color: #aab2bd;
                        /* placeholder字体大小  */
                        font-size: 12px;
                        /* placeholder位置  */
                        text-align: left;
                    }
                    &::-moz-placeholder{
                        /* placeholder颜色  */
                        color: #aab2bd;
                        /* placeholder字体大小  */
                        font-size: 12px;
                        /* placeholder位置  */
                        text-align: left;
                    }
                    &:-ms-input-placeholder{
                        /* placeholder颜色  */
                        color: #aab2bd;
                        /* placeholder字体大小  */
                        font-size: 12px;
                        /* placeholder位置  */
                        text-align: left;
                    }
                }
                .dates {
                    width: 100px;
                    display: inline-block;
                    float: left;
                }
                .datesstrong {
                    display: inline-block;
                    float: left;
                    color: #999;
                    padding: 0 10px;
                    height: 38px;
                    line-height: 38px;
                }
                select {
                    width: 100%
                }
                .selectwrap {
                    height: auto;
                    clear: both;
                    .searchable-select {
                        width: 100%;
                    }
                    .searchable-select-holder {
                        border-radius: 0;
                    }
                }
            }
            .rightmodal-footer {
                border-top: 1px solid rgb(201, 205, 208);
                text-align: center;
                padding: 15px 10px;
                overflow: hidden;
                position: absolute;
                bottom: 0;
                width: 100%
            }
        }
    }
}

.visitmodel {
    .modal-dialog {
        .modal-body {
            padding: 0
        }
        .modal-footer {
            text-align: center;
        }
    }
    .visit-wrap {
        width: 100%;
        height: auto;
        overflow: hidden;
        .visit-left,
        .visit-right {
