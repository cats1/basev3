                min-height: 300px;
                li {
                    color: #000;
                    line-height: 30px;
                    cursor: pointer;
                    input {
                        margin-right: 10px;
                    }
                    span {
                        float: right;
                    }
                }
            }
        }
    }
}

.directpage {
    width: 100%;
    width: 2480px;
    height: 3508px;
    background: #fff;
    overflow: hidden;
    box-sizing: border-box;
    .directheader {
        height: 100px;
        margin: 0px 0 0 0;
        .cardlogo {
            height: 100%;
            display: block;
            overflow: hidden;
            padding: 0 0 0 82px;
            img {
                width: 86%;
                max-height: 100px;
            }
        }
    }
    .pagecardwrap {
        width: 1016px;
        height: 664px;
        background: #fff;
        display: inline-block;
        float: left;
        margin: 124px 0 0 324px;
        .pagecardleft {
            width: auto;
            height: 100%;
            display: inline-block;
            float: left;
            .cardlogo {
                height: 132px;
                display: block;
                overflow: hidden;
                padding-left: 10px;
                img {
                    width: 280px;
                }
            }
            .cardpic {
                text-align: center;
                height: auto;
                display: block;
                padding: 34px 40px 0 100px;
                img {
                    width: 295px;
                    height: 412px;
                    border-radius: 8px;
                }
            }
        }
        .pagecardright {
            width: 529px;
            height: 100%;
            display: inline-block;
            float: left;
            .cardbar {
                width: 100px;
                height: 100%;
                background: #779fcb;
                position: relative;
                display: inline-block;
                float: left;
                ul {
                    width: 100%;
                    overflow: hidden;
                    position: absolute;
                    bottom: 66px;
                    left: 0;
                    li {
                        width: 100%;
                        height: 100px;
                        line-height: 100px;
                        text-align: center;
                        img {
                            width: 36px;
                        }
                    }
                }
            }
            .cardcons {
                display: inline-block;
                float: left;
                padding-left: 20px;
                width: 100%;
                h3 {
                    font-size: 72px;
                    color: #333;
                    margin: 26px 0 0 0;
                }
                p {
                    line-height: 32px;
                    color: #333;
                    font-size: 32px;
                    margin-top: 34px;
                    padding-left: 20px;
                    text-overflow: ellipsis;
                    overflow: hidden;
                    white-space: nowrap;
                    strong {
                        width: auto;
                        display: inline-block;
                        font-weight: 500;
                        &.worktexttip {
                            margin-right: 5px;
                        }
                        &.worktextcon {
                            max-width: 330px;
                            text-overflow: ellipsis;
                            overflow: hidden;
                            white-space: nowrap;
                        }
                    }
                    &:nth-child(2) {
                        margin-top: 30px;
                        border-left: 6px solid #ec7175
                    }
                    &:nth-child(3) {
                        border-left: 6px solid #9f70ae
                    }
                    &:nth-child(4) {
                        border-left: 6px solid #46ade3
                    }
                    &:nth-child(5) {
                        border-left: 6px solid #f8d574
                    }
                    &:nth-child(6) {
                        border-left: 6px solid #9ad2ba
                    }
                }
            }
        }
    }
}

