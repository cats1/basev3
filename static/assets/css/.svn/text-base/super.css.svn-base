#update_pwd .modal-dialog{
  width: 400px
}
#update_pwd .modal-dialog .modal-content .modal-body{
  padding: 20px 40px;
  border-radius: 0;
}
#update_pwd .modal-dialog .modal-content .form-group {
  margin:5px 0
}
#update_pwd .modal-dialog .modal-content .form-group.last{
  margin-top:15px
}
#update_pwd .modal-dialog .modal-content .form-group label{
  height: 37px;
  line-height: 37px
}
#update_pwd .modal-dialog .modal-content .form-group input {
  height: 40px;
}
#update_pwd .modal-dialog .modal-content .form-group label .dot {
  width: 20px;
  height: 8px;
  background: url(../img/dot.png) no-repeat center;
  display: inline-block;
  margin-bottom: 2px;
  background-size: 5px;
}
#update_pwd .modal-dialog .modal-content .options-wrap .row {
  margin-left: -6px;
  margin-right: -6px;
}
#update_pwd .modal-dialog .modal-content .options-wrap .row .col-md-3,
#update_pwd .modal-dialog .modal-content .options-wrap .row .col-md-9 {
  padding: 0 6px;
}
#update_pwd .modal-dialog .modal-content .options-wrap .option {
  border: 1px solid #cccccc;
  color: #666;
  text-align: center;
  line-height: 40px;
}