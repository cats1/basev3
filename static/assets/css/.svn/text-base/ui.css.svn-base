::selection {
  background: rgba(47, 141, 246, 0.3);
  color: #666666;
  text-shadow: none;
}
a:hover,
a:focus,
a:active {
  text-decoration: none;
}
.left {
  float: left;
}
.right {
  float: right;
}
.check {
  border-radius: 90px;
  width: 72px;
  height: 47px;
  overflow: hidden;
  position: relative;
  cursor: pointer;
}
.check .switch {
  height: 37px;
  width: 37px;
  background: #fff;
  margin: 5px;
  border-radius: 90px;
  position: absolute;
}
.check.checked {
  background: #67bf6b;
}
.check.checked .switch {
  left: 25px;
}
.check.unchecked {
  background: #cccccc;
}
.check.unchecked .switch {
  left: 0px;
}
.form-group {
  position: relative;
}
.form-group .error-msg {
  position: absolute;
  right: 15px;
  bottom: 14px;
  color: #d97f7f;
}
.form-group .row .error-msg {
  right: 30px;
}
.form-group .row .warning-msg {
  right: 30px;
}
.form-group .warning-msg {
  position: absolute;
  right: 15px;
  bottom: 14px;
  color: #c19a23;
}
.form-group .form-control.error {
  border-color: #d97f7f;
}
.form-group .form-control.warning {
  border-color: #c19a23;
}
.form-control[disabled],
.form-control[readonly],
fieldset[disabled] .form-control {
  cursor: not-allowed;
  background-color: #f5f5f5;
  opacity: 1;
}
.btn-default {
  border-radius: 100px;
  color: #fff;
  background: #67bf6b;
  border: none;
  outline: none !important;
  height: 47px;
  line-height: 47px;
  padding: 0 30px;
  font-size: 18px;
}
.btn-default:hover,
.btn-default:focus {
  color: #fff;
  background: #79c77c;
}
.btn-bordered {
  border: 1px solid #67bf6b;
  color: #67bf6b;
  background: none;
}
.btn-bordered:hover,
.btn-bordered:focus {
  background: none;
  color: #afddb1;
  border-color: #afddb1;
}
.btn-small {
  height: 38px;
  line-height: 38px;
  padding: 0 18px;
  font-size: 16px;
}
.form-control {
  border-radius: 0;
  height: 47px;
  box-shadow: none;
  font-size: 16px;
}
.form-control:focus {
  box-shadow: none;
}
.form-group label {
  font-size: 16px;
  font-weight: normal;
  color: #999999;
}
.tooltip.bottom {
  margin-top: 9px;
}
.tooltip.bottom .tooltip-arrow {
  width: 18px;
  height: 9px;
  background: url(../img/arrow.png) no-repeat center;
  border: none;
  margin-left: -9px;
}
.tooltip.bottom .tooltip-inner {
  background: rgba(0, 0, 0, 0.9);
  border-radius: 0;
  font-size: 14px;
  margin-top: 4px;
  padding: 10px 15px;
}
.modal .modal-backdrop.in {
  opacity: .35;
}
.modal .modal-dialog {
  width: 700px;
  margin: 130px auto;
  background: rgba(0,0,0,0.4);
  padding:16px;
}
.modal .modal-backdrop.in {
  opacity: .35;
}
.modal .modal-dialog {
  width: 700px;
  margin: 130px auto;
  background: rgba(0,0,0,0.4);
  padding:16px;
}
.modal .modal-dialog .modal-content {
  padding: 0;
  border-radius: 0;
}
.modal .modal-dialog .modal-content p {
  font-size: 16px;
}
.modal .modal-dialog .modal-header {
  padding: 0;
  border-bottom: none;
  width:100%;
  height:49px;
  background-color: rgb( 224, 224, 224 );
}
.modal .modal-dialog .modal-header .close {
  position: absolute;
  background: url(../img/close_small.png) no-repeat center;
  background-size:100% 100%; 
  width: 14px;
  height: 14px;
  opacity: 1;
  outline: 0;
  right: 30px;
  top: 18px;
}
.modal .modal-dialog .modal-header .close span {
  display: none;
}
.modal .modal-dialog .modal-header .close:hover {
  opacity: .7;
}
.modal .modal-dialog .modal-content .modal-header h4{
  line-height: 49px;
  text-align: center;
  font-size:18px;
  color: #444
}
.modal .modal-dialog .modal-body {
  padding: 0;
  background: #fff;
  min-height: 50px;
  padding:50px 0;
}
.modal .modal-dialog .modal-body img {
  max-width: 100%;
}
.modal .modal-dialog .modal-content .modal-body h4{
  font-size:14px;
  color:#999;
}
a[data-toggle='modal'] {
  outline: 0;
}
.alert {
  margin-top: 20px;
  font-size: 16px;
  line-height: 20px;
  border-radius: 0;
  color: #fff;
  border: none;
}
.alert .close {
  font-size: 16px;
  font-weight: normal;
  line-height: 20px;
  text-shadow: none;
  background: url(../img/close_small_white.png) no-repeat center;
  width: 20px;
  height: 20px;
  opacity: .8;
}
.alert .close:hover {
  opacity: .6;
}
.alert .close .glyphicon {
  display: none;
}
.alert .alert-link,
.alert a {
  color: #fff;
  border-bottom: 1px solid #fff;
  font-weight: normal;
}
.alert-danger {
  background: #f55567;
}
.alert-success {
  background: #67bf6b;
}
.alert-fixed {
  position: fixed;
  top: 0px;
  width: 1050px;
  z-index: 9999;
  margin: 0;
}
#datepicker {
  width: auto;
  padding: 0;
  width: 290px;
}
#datepicker .ui-datepicker {
  width: auto;
  padding: 0;
}
#datepicker .ui-widget-content {
  background: transparent !important;
  border: none !important;
}
#datepicker .ui-widget-header {
  border: none;
  background: transparent;
  color: #666666;
  border-radius: 0;
}
#datepicker .ui-widget-header a.ui-corner-all {
  top: 5px;
  left: 0;
  cursor: pointer;
}
#datepicker .ui-widget-header a.ui-corner-all:hover {
  border: none;
  background: transparent;
}
#datepicker .ui-widget-header a.ui-corner-all .ui-icon {
  background: url(../img/left2.png) no-repeat;
  background-size: 10px !important;
  background-position: left;
  width: 20px;
  height: 20px;
  cursor: pointer;
}
#datepicker .ui-widget-header a.ui-corner-all .ui-icon:hover {
  background: url(../img/left2.png) no-repeat;
  background-size: 10px !important;
  background-position: left;
  opacity: .7;
  border: none;
}
#datepicker .ui-widget-header a.ui-datepicker-next {
  left: auto;
  right: 0 !important;
}
#datepicker .ui-widget-header a.ui-datepicker-next .ui-icon {
  background: url(../img/right2.png) no-repeat;
  background-size: 10px !important;
  background-position: right;
}
#datepicker .ui-widget-header a.ui-datepicker-next .ui-icon:hover {
  background: url(../img/right2.png) no-repeat;
  background-size: 10px !important;
  background-position: right;
  opacity: .7;
  border: none;
}
#datepicker .ui-widget {
  font-family: Arial, sans-serif;
}
#datepicker .ui-datepicker-header {
  padding-bottom: 15px;
}
#datepicker .ui-datepicker .ui-datepicker-title {
  font-weight: normal;
  color: #566ebb;
  font-size: 18px;
  border-radius: 0;
}
#datepicker .ui-datepicker th {
  font-weight: normal;
  font-size: 16px;
  color: #999999;
}
#datepicker table tr {
  border-top: 1px solid #cccccc;
}
#datepicker .ui-state-default,
#datepicker .ui-widget-content .ui-state-default,
#datepicker .ui-widget-header .ui-state-default {
  border: none;
  background: transparent;
  color: #999999;
  font-size: 14px;
  font-weight: normal;
  padding: 5px 8px;
  text-align: center;
  margin: 5px 3px;
}
#datepicker .ui-state-default:hover,
#datepicker .ui-widget-content .ui-state-default:hover,
#datepicker .ui-widget-header .ui-state-default:hover {
  color: #fff;
  background: #aabaef;
}
#datepicker .ui-state-highlight,
#datepicker .ui-widget-content .ui-state-highlight,
#datepicker .ui-widget-header .ui-state-highlight {
  color: #333333;
  font-weight: bold;
}
#datepicker .ui-state-active,
#datepicker .ui-widget-content .ui-state-active,
#datepicker .ui-widget-header .ui-state-active {
  color: #fff !important;
  background: #566ebb !important;
}
