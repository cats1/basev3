/* button */
.blue-button1{
  border-radius: 3px;
  background-image: -moz-linear-gradient( 90deg, rgb(34,116,230) 0%, rgb(48,145,242) 100%);
  background-image: -webkit-linear-gradient( 90deg, rgb(34,116,230) 0%, rgb(48,145,242) 100%);
  background-image: -ms-linear-gradient( 90deg, rgb(34,116,230) 0%, rgb(48,145,242) 100%);
  text-shadow: 0.5px 0.866px 0px rgba( 8, 1, 3,.3);
  width: auto;
  height: 37px;
  color: #fff;
  padding: 0px 15px;
}
.blue-button1:hover{
  border-radius: 3px;
  background-image: -moz-linear-gradient( 90deg, rgb(29,96,191) 0%, rgb(41,122,204) 100%);
  background-image: -webkit-linear-gradient( 90deg, rgb(29,96,191) 0%, rgb(41,122,204) 100%);
  background-image: -ms-linear-gradient( 90deg, rgb(29,96,191) 0%, rgb(41,122,204) 100%);
  width: auto;
  height: 37px;
  color: #fff
}
.blue-button1:active{
  border-style: 1px solid rgb( 212, 212, 212 );
  border-radius: 3px;
  background-image: -moz-linear-gradient( 90deg, rgb(217,217,217) 0%, rgb(224,224,224) 100%);
  background-image: -webkit-linear-gradient( 90deg, rgb(217,217,217) 0%, rgb(224,224,224) 100%);
  background-image: -ms-linear-gradient( 90deg, rgb(217,217,217) 0%, rgb(224,224,224) 100%);
  color: #fff
}
.blue-button1:focus{
  border-radius: 3px;
  background-image: -moz-linear-gradient( 90deg, rgb(34,116,230) 0%, rgb(48,145,242) 100%);
  background-image: -webkit-linear-gradient( 90deg, rgb(34,116,230) 0%, rgb(48,145,242) 100%);
  background-image: -ms-linear-gradient( 90deg, rgb(34,116,230) 0%, rgb(48,145,242) 100%);
  color: #fff
}
